{
  "name": "@borderless/site",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Server side rendering for react.js applications",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/borderless/site.git"
  },
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "homepage": "https://github.com/borderless/site",
  "bugs": {
    "url": "https://github.com/borderless/site/issues"
  },
  "bin": "./dist/bin.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dev": "./dist/dev.js",
    "./head": "./dist/head.js",
    "./render/client": "./dist/render/client.js",
    "./render/server": "./dist/render/server.js",
    "./components/404": "./dist/components/404.js",
    "./components/error": "./dist/components/error.js",
    "./components/app": "./dist/components/app.js",
    "./components/document": "./dist/components/document.js"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "example:test": "npm run site -- --root examples/test",
    "format": "ts-scripts format",
    "lint": "ts-scripts lint",
    "prepare": "ts-scripts install && ts-scripts build",
    "site": "node --loader ts-node/esm src/bin.ts",
    "specs": "ts-scripts specs",
    "test": "ts-scripts test"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "server",
    "render",
    "static",
    "site",
    "render",
    "react",
    "reactjs"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@borderless/router": "^1.0.4",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "arg": "^5.0.0",
    "chokidar": "^3.5.2",
    "iterative": "^1.9.2",
    "react-helmet-async": "^1.0.9",
    "vite": "^2.3.8"
  },
  "devDependencies": {
    "@borderless/ts-scripts": "^0.5.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^16.4.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "typings": "./dist/index.d.ts",
  "ts-scripts": {
    "project": [
      "tsconfig.build.json"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  }
}
